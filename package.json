{
  "name": "@johanbook/eslint-plugin-commented-exports",
  "version": "0.0.1",
  "description": "Forces exports to be commented",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Johan Book",
  "main": "lib/index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "release": "standard-version",
    "test": "mocha tests --recursive"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "eslint": "^7.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mocha": "^7.2.0",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  }
}
